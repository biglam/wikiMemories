<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Placetype</th>
<!--       <th>Lat</th>
      <th>Lng</th> -->
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user.administered_places.each do |place| %>
      <tr>
        <td><%= place.name %></td>
        <td><%= place.placetype.placetype unless place.placetype == nil %></td>
        <td><%= link_to 'Show', place, class: "btn btn-info" %></td>
        <td><%#= link_to 'Edit', edit_place_path(place) %></td>
        <td><%= link_to 'Destroy', place, method: :delete, data: { confirm: 'Are you sure?' } if current_user && current_user.role == "owner" %></td>
      </tr>
    <% end %>
  </tbody>
</table>