
<div class="memorydiv">
    <div class="row">
        <div class="memoryleftpanel">
            <%= render partial: 'layouts/memory_rank', locals: {memory: memory} %>
        </div>

        <div class="memoryrightpanel">
            <%= render partial: 'layouts/memory_details', locals: {memory: memory} %>
            <%= render partial: 'layouts/memory_options', locals: {memory: memory} %>

            <%= render partial: 'layouts/memory_flag', locals: {memory: memory} %>

            <% if frompage == "flagpage" %>
                <%= render partial: 'layouts/memory_flag_details', locals: {memory: memory, frompage: frompage} %>
            <% end %>
        </div>
    </div>
</div>
